<?xml version="1.0" encoding="UTF-8"?>
<root>
    <ApplicationConfiguration>
        <ApplicationName>CEL Commissionator</ApplicationName>
        <ApplicationDataStorageDirectory>$APPDATA_HOME$/#ApplicationConfiguration:ApplicationName#/</ApplicationDataStorageDirectory>
        <UserDataStorageDirectory>$USER_HOME$/#ApplicationConfiguration:ApplicationName#/</UserDataStorageDirectory>
        <requiredFirmwareVersion>657</requiredFirmwareVersion>
        <DefaultMode>Normal</DefaultMode>
        <DefaultLocaleLanguageTag>en-GB</DefaultLocaleLanguageTag>
    </ApplicationConfiguration>
    <Stenographer>
       <logfilename>#ApplicationConfiguration:ApplicationDataStorageDirectory#/Commissionator.log</logfilename>
       <logmode>LOCAL</logmode>
       <logtee>T</logtee>
       <loglevel>INFO</loglevel>
    </Stenographer>
</root>
