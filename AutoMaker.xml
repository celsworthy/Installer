<project>
    <shortName>AutoMaker</shortName>
    <fullName>CEL AutoMaker</fullName>
    <version>999</version>
    <readmeFile>README_AutoMaker.txt</readmeFile>
    <licenseFile>EULA_AutoMaker.txt</licenseFile>
    <leftImage>LeftImage - AutoMaker 163x314.png</leftImage>
    <splashImage>Splash - AutoMaker 600x400.png</splashImage>
    <wmImage>RoboxIcon48x48.png</wmImage>
    <componentList>
        <component>
            <name>windowsdrivers</name>
            <description>Windows Drivers</description>
            <canBeEdited>1</canBeEdited>
            <selected>1</selected>
            <show>1</show>
            <folderList>
                <folder>
                    <description>Windows Drivers</description>
                    <destination>${commondir}</destination>
                    <name>windows_drivers</name>
                    <platforms>windows</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>windows_drivers</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
            </folderList>
            <postInstallationActionList>
                <runProgram>
                    <abortOnError>0</abortOnError>
                    <explanation>Installing windows drivers for Robox</explanation>
                    <program>${commondir}\windows_drivers\dpinst_x86.exe</program>
                    <programArguments></programArguments>
                    <progressText>Installing windows drivers for Robox</progressText>
                    <showMessageOnError>0</showMessageOnError>
                    <ruleList>
                        <platformTest>
                            <type>windows-x86</type>
                        </platformTest>
                    </ruleList>
                </runProgram>
                <runProgram>
                    <abortOnError>0</abortOnError>
                    <explanation>Installing windows drivers for Robox</explanation>
                    <program>${commondir}\windows_drivers\dpinst_x64.exe</program>
                    <programArguments></programArguments>
                    <progressText>Installing windows drivers for Robox</progressText>
                    <showMessageOnError>0</showMessageOnError>
                    <ruleList>
                        <platformTest>
                            <type>windows-x64</type>
                        </platformTest>
                    </ruleList>
                </runProgram>
            </postInstallationActionList>
        </component>
        <component>
            <name>Application</name>
            <description>Application</description>
            <canBeEdited>1</canBeEdited>
            <selected>1</selected>
            <show>1</show>
            <desktopShortcutList>
                <shortcut>
                    <comment>AutoMaker</comment>
                    <exec>${automakerdir}/${project.shortName}.${platform_exec_suffix}</exec>
                    <icon>${automakerdir}/AutoMaker.ico</icon>
                    <name>AutoMaker</name>
                    <path></path>
                    <platforms>all</platforms>
                    <runAsAdmin>0</runAsAdmin>
                    <runInTerminal>0</runInTerminal>
                    <windowsExec>${automakerdir}/${project.shortName}.${platform_exec_suffix}</windowsExec>
                    <windowsExecArgs></windowsExecArgs>
                    <windowsIcon>${automakerdir}/AutoMaker.ico</windowsIcon>
                    <windowsPath></windowsPath>
                    <ruleList>
                        <isTrue>
                            <value>${install_desktop_shortcut}</value>
                        </isTrue>
                    </ruleList>
                </shortcut>
            </desktopShortcutList>
            <folderList>
                <folder>
                    <description>Program Files</description>
                    <destination>${automakerdir}</destination>
                    <name>programfiles</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>AutoMaker.configFile.xml</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>../JarArtifacts/AutoMaker.jar</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>EULA_AutoMaker.txt</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>README_AutoMaker.txt</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>../JarArtifacts/AutoMakerMigrator.jar</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files x86</description>
                    <destination>${automakerdir}</destination>
                    <name>programfileslinuxx86</name>
                    <platforms>linux</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>AutoMaker-update-linux.run</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMaker.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMakerUninstallIcon.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>librxtxSerial.so</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>runMigrator.sh</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files x64</description>
                    <destination>${automakerdir}</destination>
                    <name>programfileslinuxx64</name>
                    <platforms>linux-x64</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>AutoMaker-update-linux-x64.run</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMaker.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMakerUninstallIcon.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>librxtxSerial.so</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>runMigrator.sh</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${automakerdir}</destination>
                    <name>programfileswindows</name>
                    <platforms>windows</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>AutoMaker-update-windows.exe</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMaker.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMakerUninstallIcon.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>rxtxSerial.dll</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>rxtxSerial64.dll</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>jRegistryKey.dll</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>runMigrator.bat</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${automakerdir}</destination>
                    <name>programfilesosx</name>
                    <platforms>osx</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>AutoMaker-update-osx.app</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMaker.icns</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMakerUninstallIcon.icns</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>AutoMaker.png</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>librxtxSerial.jnilib</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>runMigrator.sh</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
            </folderList>
            <startMenuShortcutList>
                <startMenuShortcut>
                    <comment>AutoMaker</comment>
                    <name>AutoMaker</name>
                    <runAsAdmin>0</runAsAdmin>
                    <runInTerminal>0</runInTerminal>
                    <windowsExec>${automakerdir}/${project.shortName}.${platform_exec_suffix}</windowsExec>
                    <windowsExecArgs></windowsExecArgs>
                    <windowsIcon>${automakerdir}/AutoMaker.ico</windowsIcon>
                    <windowsPath></windowsPath>
                    <ruleList>
                        <isTrue>
                            <value>${install_menu_shortcut}</value>
                        </isTrue>
                    </ruleList>
                </startMenuShortcut>
            </startMenuShortcutList>
        </component>
        <include>
            <file>java.xml</file>
        </include>
        <component>
            <name>Common</name>
            <description>Common files</description>
            <canBeEdited>0</canBeEdited>
            <detailedDescription>Common files required by all CEL applications</detailedDescription>
            <selected>1</selected>
            <show>0</show>
            <folderList>
                <folder>
                    <description>Program Files</description>
                    <destination>${commondir}</destination>
                    <name>common</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>EULA_${project.shortName}.txt</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>README_${project.shortName}.txt</origin>
                        </distributionFile>
                        <distributionFile>
                            <allowWildcards>1</allowWildcards>
                            <origin>robox_r*.bin</origin>
                        </distributionFile>
                        <distributionDirectory>
                            <origin>Macros</origin>
                        </distributionDirectory>
                        <distributionDirectory>
                            <origin>PrintProfiles</origin>
                        </distributionDirectory>
                        <distributionDirectory>
                            <origin>Heads</origin>
                        </distributionDirectory>
                        <distributionDirectory>
                            <origin>Filaments</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files x86</description>
                    <destination>${commondir}</destination>
                    <name>programfileslinuxx86</name>
                    <platforms>linux</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>Slicer/Linux_x86/Slic3r</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files x64</description>
                    <destination>${commondir}</destination>
                    <name>programfileslinuxx64</name>
                    <platforms>linux-x64</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>Slicer/Linux_x64/Slic3r</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${commondir}</destination>
                    <name>programfileswindows</name>
                    <platforms>windows</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>Slicer/Windows/Slic3r</origin>
                        </distributionDirectory>
                        <distributionFile>
                            <origin>icons/STL File.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>icons/OBJ File.ico</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>icons/RBX File.ico</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Program Files</description>
                    <destination>${commondir}</destination>
                    <name>programfilesosx</name>
                    <platforms>osx</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>Slicer/Mac/Slic3r.app</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Binary files - mac</description>
                    <destination>${commondir}/bin</destination>
                    <name>bin_mac</name>
                    <platforms>osx</platforms>
                    <actionList>
                        <changePermissions files="${commondir}/bin/*" permissions="0755"/>
                    </actionList>
                    <distributionFileList>
                        <distributionFile>
                            <origin>bin/RoboxDetector.mac.sh</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Binary files - windows</description>
                    <destination>${commondir}/bin</destination>
                    <name>bin_windows</name>
                    <platforms>windows</platforms>
                    <actionList>
                        <changePermissions files="${commondir}/bin/*" permissions="0755"/>
                    </actionList>
                    <distributionFileList>
                        <distributionFile>
                            <origin>bin/RoboxDetector.exe</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>bin/msvcp100.dll</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>bin/msvcr100.dll</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Binary files - linux</description>
                    <destination>${commondir}/bin</destination>
                    <name>bin_linux</name>
                    <platforms>linux</platforms>
                    <actionList>
                        <changePermissions files="${commondir}/bin/*" permissions="0755"/>
                    </actionList>
                    <distributionFileList>
                        <distributionFile>
                            <origin>bin/RoboxDetector.linux.sh</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Binary files - linux x64</description>
                    <destination>${commondir}/bin</destination>
                    <name>bin_linux_x64</name>
                    <platforms>linux-x64</platforms>
                    <actionList>
                        <changePermissions files="${commondir}/bin/*" permissions="0755"/>
                    </actionList>
                    <distributionFileList>
                        <distributionFile>
                            <origin>bin/RoboxDetector.linux.sh</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
            </folderList>
        </component>
    </componentList>
    <postBuildActionList>
        <runProgram>
            <program>C:\Program Files (x86)\Windows Kits\8.0\bin\x86\signtool</program>
            <programArguments>sign /d "${project.fullName}" /f D:\Development\CELCodeSigningCert.p12 /p bl1nk3r3d /t http://timestamp.verisign.com/scripts/timestamp.dll "${project.outputDirectory}/${project.installerFilename}"</programArguments>
            <ruleList>
                <platformTest>
                    <type>windows-x86</type>
                </platformTest>
            </ruleList>
        </runProgram>
        <runProgram>
            <program>C:\Program Files (x86)\Windows Kits\8.0\bin\x64\signtool</program>
            <programArguments>sign /d "${project.fullName}" /f D:\Development\CELCodeSigningCert.p12 /p bl1nk3r3d /t http://timestamp.verisign.com/scripts/timestamp.dll "${project.outputDirectory}/${project.installerFilename}"</programArguments>
            <ruleList>
                <platformTest>
                    <type>windows-x64</type>
                </platformTest>
            </ruleList>
        </runProgram>
    </postBuildActionList>
    <initializationActionList>
        <setInstallerVariable>
            <name>vendor</name>
            <value>CEL</value>
        </setInstallerVariable>
        <setInstallerVariable>
            <name>installdir</name>
            <value>${platform_install_prefix}/${vendor}/</value>
        </setInstallerVariable>
        <setInstallerVariable>
            <name>launcherName</name>
            <value>AutoMaker</value>
        </setInstallerVariable>
        <setInstallerVariable>
            <name>install_desktop_shortcut</name>
            <value>0</value>
        </setInstallerVariable>
        <setInstallerVariable>
            <name>install_menu_shortcut</name>
            <value>0</value>
        </setInstallerVariable>
        <setInstallerVariable>
            <name>update_xml_url</name>
            <value>http://downloads.cel-robox.com:8001/0abc523fc24/AutoMaker-update.xml</value>
        </setInstallerVariable>
        <setInstallerVariableFromRegEx>
            <name>installerVersionID</name>
            <pattern>\.</pattern>
            <substitution></substitution>
            <text>${product_version}</text>
        </setInstallerVariableFromRegEx>
        <setInstallerVariable>
            <name>windowsDriverDir</name>
            <value>${installdir}/${project.shortName}/windows_drivers</value>
        </setInstallerVariable>
    </initializationActionList>
    <preInstallationActionList>
        <actionGroup>
            <actionList>
                <setInstallerVariable name="allowComponentSelection" value="0"/>
                <setInstallerVariable name="installationType" value="upgrade"/>
                <componentSelection deselect="windowsdrivers">
                    <select></select>
                </componentSelection>
            </actionList>

            <!-- We assume an existing installation if ${installdir} directory exists -->
            <ruleList>
                <fileTest condition="exists" path="${installdir}"/>
            </ruleList>
        </actionGroup>
    </preInstallationActionList>
    <postInstallationActionList>
        <writeFile>
            <path>${automakerdir}/application.properties</path>
            <text>version = ${project.version}
language = ${installation_language_code}</text>
        </writeFile>
        <writeFile>
            <path>${automakerdir}/update.ini</path>
            <text>[Update]
url = ${update_xml_url}
version_id = ${installerVersionID}
update_download_location = ${installdir}/
check_for_updates = 1
[Proxy]
enable = 0
</text>
        </writeFile>
        <addDirectoriesToUninstaller>
            <addContents>1</addContents>
            <files>${windowsDriverDir}</files>
            <ruleList>
                <platformTest>
                    <type>windows</type>
                </platformTest>
            </ruleList>
        </addDirectoriesToUninstaller>
        <addFilesToUninstaller>
            <files>${automakerdir}/update.ini</files>
        </addFilesToUninstaller>
        <addFilesToUninstaller>
            <files>${automakerdir}/application.properties</files>
        </addFilesToUninstaller>
        <addDirectoriesToUninstaller>
            <addContents>1</addContents>
            <files>${windows_folder_appdata}\${product_fullname}</files>
            <ruleList>
                <platformTest>
                    <type>windows</type>
                </platformTest>
            </ruleList>
        </addDirectoriesToUninstaller>
        <runProgram>
            <program>defaults</program>
            <programArguments>write "/${automakerdir}/${project.shortName}.app/Contents/Info" CFBundleIdentifier com.CEL.${project.shortName}</programArguments>
            <ruleList>
                <platformTest>
                    <type>osx</type>
                </platformTest>
            </ruleList>
        </runProgram>
        <runProgram>
            <program>defaults</program>
            <programArguments>write "/${automakerdir}/${project.shortName}.app/Contents/Info" CFBundleName ${project.shortName}</programArguments>
            <ruleList>
                <platformTest>
                    <type>osx</type>
                </platformTest>
            </ruleList>
        </runProgram>
        <runProgram>
            <program>plutil</program>
            <programArguments>-convert xml1 "${automakerdir}/${project.shortName}.app/Contents/Info.plist" </programArguments>
            <ruleList>
                <platformTest>
                    <type>osx</type>
                </platformTest>
            </ruleList>
        </runProgram>
        <actionGroup>
            <actionList>
                <runProgram>
                    <program>defaults</program>

                    <!-- The CDATA notation allow escaping a
            block of XML characters -->
                    <programArguments>write com.apple.LaunchServices LSHandlers -array-add "&lt;dict&gt;&lt;key&gt;LSHandlerContentTag&lt;/key&gt;
&lt;string&gt;${projectFileExtension}&lt;/string&gt;&lt;key&gt;LSHandlerContentTagClass&lt;/key&gt;
&lt;string&gt;public.filename-extension&lt;/string&gt;&lt;key&gt;LSHandlerRoleAll&lt;/key&gt;
&lt;string&gt;com.CELTech.AutoMaker&lt;/string&gt;&lt;/dict&gt;"
           </programArguments>
                    <ruleList>
                        <platformTest>
                            <type>osx</type>
                        </platformTest>
                    </ruleList>
                </runProgram>

                <!-- Restart the launch services to reload the configuration -->
                <runProgram>
                    <program>/System/Library/Frameworks/CoreServices.framework/Versions/A/Frameworks/LaunchServices.framework/Versions/A/Support/lsregister</program>
                    <programArguments>-kill -domain local -domain system -domain user</programArguments>
                    <ruleList>
                        <platformTest>
                            <type>osx</type>
                        </platformTest>
                    </ruleList>
                </runProgram>
            </actionList>
        </actionGroup>
        <runProgram>
            <explanation>Migrating last installation</explanation>
            <program>runMigrator.bat</program>
            <programArguments></programArguments>
            <workingDirectory>${installdir}/${project.shortName}</workingDirectory>
            <ruleList>
                <platformTest>
                    <type>windows</type>
                </platformTest>
            </ruleList>
        </runProgram>
        <runProgram>
            <explanation>Migrating last installation</explanation>
            <program>./runMigrator.sh</program>
            <programArguments></programArguments>
            <workingDirectory>${installdir}/${project.shortName}</workingDirectory>
            <ruleList>
                <platformTest>
                    <negate>1</negate>
                    <type>windows</type>
                </platformTest>
            </ruleList>
        </runProgram>
    </postInstallationActionList>
    <postUninstallationActionList>
        <actionGroup>
            <actionList>
                <registryDelete>
                    <key>HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Uninstall\${project.vendor} ${project.fullName} ${project.version}</key>
                    <name></name>
                </registryDelete>
            </actionList>
            <ruleList>
                <platformTest>
                    <type>windows</type>
                </platformTest>
            </ruleList>
        </actionGroup>
    </postUninstallationActionList>
    <allowedLanguages>en ja ko ru zh_CN</allowedLanguages>
    <compressionAlgorithm>lzma</compressionAlgorithm>
    <enableRollback>0</enableRollback>
    <enableTimestamp>1</enableTimestamp>
    <installationScope>user</installationScope>
    <osxApplicationBundleIcon>AutoMaker.icns</osxApplicationBundleIcon>
    <osxUninstallerApplicationBundleIcon>AutoMaker.icns</osxUninstallerApplicationBundleIcon>
    <outputDirectory>LatestBuilds</outputDirectory>
    <productDisplayIcon>${automakerdir}/AutoMaker.ico</productDisplayIcon>
    <productUrlInfoAbout>www.cel-robox.com</productUrlInfoAbout>
    <requireInstallationByRootUser>1</requireInstallationByRootUser>
    <saveRelativePaths>1</saveRelativePaths>
    <showFileUnpackingProgress>0</showFileUnpackingProgress>
    <splashScreenDelay>3000</splashScreenDelay>
    <startMenuGroupName>${vendor}</startMenuGroupName>
    <style>custom</style>
    <uninstallerName>Uninstall AutoMaker</uninstallerName>
    <vendor>C Enterprise (UK) Limited</vendor>
    <width>600</width>
    <windows64bitMode>1</windows64bitMode>
    <windowsExecutableIcon>AutoMaker.ico</windowsExecutableIcon>
    <windowsUninstallerExecutableIcon>AutoMakerUninstallIcon.ico</windowsUninstallerExecutableIcon>
    <finalPageActionList>
        <runProgram>
            <!-- On Mac OS X, use 'open' command for asynchronous launch -->
            <program>open</program>
            <programArguments>${automakerdir}/${launcherName}.app</programArguments>
            <progressText>Do you want to launch ${product_fullname} now?</progressText>
            <ruleList>
                <platformTest type="osx"/>
            </ruleList>
        </runProgram>
        <runProgram>
            <!-- On Windows, use ampersand for asynchronous launch -->
            <program>${automakerdir}/${launcherName}.exe</program>
            <programArguments>&amp;</programArguments>
            <progressText>Do you want to launch ${product_fullname} now?</progressText>
            <ruleList>
                <platformTest type="windows"/>
            </ruleList>
        </runProgram>
        <runProgram>
            <program>${automakerdir}/${launcherName}.run</program>
            <programArguments>&amp;</programArguments>
            <progressText>Do you want to launch ${product_fullname} now?</progressText>
            <ruleList>
                <platformTest type="linux"/>
            </ruleList>
        </runProgram>
    </finalPageActionList>
