            <explanation>Please specify the directory where CEL products will be installed</explanation>
            <value></value>
            <default>${platform_install_prefix}/${product_shortname}-${product_version}</default>
            <allowEmptyValue>0</allowEmptyValue>
            <ask>yes</ask>
            <cliOptionName>prefix</cliOptionName>
            <mustBeWritable>yes</mustBeWritable>
            <mustExist>0</mustExist>
            <width>30</width>
            <postShowPageActionList>
                <setInstallerVariable>
                    <name>automakerdir</name>
                    <value>${installdir}/${project.shortName}/</value>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>java_launcher_jar</name>
                    <value>${installdir}/${project.shortName}/AutoMaker.jar</value>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>commondir</name>
                    <value>${installdir}/Common</value>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>java_launcher_jar</name>
                    <value>AutoMaker.jar</value>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>java_launcher_vm_parameters</name>
                    <value>-XX:-OmitStackTraceInFastThrow -Xms512m -Xmx750m -DlibertySystems.configFile=AutoMaker.configFile.xml</value>
                    <ruleList>
                        <platformTest>
                            <negate>1</negate>
                            <type>osx</type>
                        </platformTest>
                    </ruleList>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>java_launcher_vm_parameters</name>
                    <value>-XX:-OmitStackTraceInFastThrow -Xms512m -Xmx750m -DlibertySystems.configFile=AutoMaker.configFile.xml -Xdock:icon=${automakerdir}/AutoMaker.png "-Xdock:name=AutoMaker"</value>
                    <ruleList>
                        <platformTest>
                            <type>osx</type>
                        </platformTest>
                    </ruleList>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>java_launcher_binary_name</name>
                    <value>${project.shortName}.${platform_exec_suffix}</value>
                </setInstallerVariable>
                <setInstallerVariable>
                    <name>java_launcher_startmenu_shortcut_name</name>
                    <value>${project.shortName}</value>
                </setInstallerVariable>
            </postShowPageActionList>
